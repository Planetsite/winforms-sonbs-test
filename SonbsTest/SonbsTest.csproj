    <PackageReference Include="Microsoft.Extensions.Configuration.UserSecrets" Version="8.0.1" />
    <PackageReference Include="Planet.Core" Version="7.7.0" />
    <PackageReference Include="Planet.IntegrationEvents.Domain" Version="7.6.4" />
    <PackageReference Include="Planet.Sonbs.TcpClient" Version="7.1.1" />
